<brls:Box
        axis="column"
        width="auto"
        margin="40"
        height="auto">
    <brls:Header
            id="setting/net/test/header"
            title="@i18n/tsvitch/setting/net/test/header"
            marginBottom="@style/tsvitch/margin/20"/>
    <brls:Box
            marginLeft="20"
            marginRight="20">
        <brls:Label
                grow="1"
                text="@i18n/tsvitch/setting/net/test/feed"/>
        <brls:Label
                id="setting/net/test1"
                horizontalAlign="right"
                text="..."/>
    </brls:Box>
    <brls:Header
            title="@i18n/tsvitch/setting/net/profile/header"
            subtitle="@i18n/tsvitch/setting/net/profile/subtitle"
            marginBottom="@style/tsvitch/margin/20"
            marginTop="@style/tsvitch/margin/20"/>
    <brls:Box
            marginBottom="20"
            marginLeft="20"
            marginRight="20">
        <brls:Label
                grow="1"
                text="WIFI"/>
        <brls:Label
                id="setting/net/wifi"
                horizontalAlign="right"
                text="@i18n/hints/on"/>
    </brls:Box>
    <brls:Box
            marginBottom="@style/tsvitch/margin/20"
            marginLeft="20"
            marginRight="20">
        <brls:Label
                grow="1"
                text="@i18n/tsvitch/setting/net/profile/ip"/>
        <brls:Label
                id="setting/net/ip"
                horizontalAlign="right"
                text="-.-.-.-"/>
    </brls:Box>
    <brls:Box
            alignItems="flexStart"
            marginLeft="20"
            marginRight="20">
        <brls:Label
                grow="1"
                text="@i18n/tsvitch/setting/net/profile/dns"/>
        <brls:Label
                id="setting/net/dns"
                horizontalAlign="right"
                text="-.-.-.-"/>
    </brls:Box>

</brls:Box>